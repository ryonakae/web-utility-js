{
  "name": "web-utility-js",
  "version": "0.0.4",
  "description": "JavaScript utilities for web develop.",
  "main": "lib/index.js",
  "scripts": {
    "build": "NODE_ENV=production babel src -d lib -s inline",
    "watch": "babel src -d lib -s inline -w",
    "format": "prettier 'src/**/*.js' --write",
    "prepublish": "npm run format && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryonakae/web-utility-js.git"
  },
  "author": "Ryo Nakae",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ryonakae/web-utility-js/issues"
  },
  "homepage": "https://github.com/ryonakae/web-utility-js#readme",
  "dependencies": {
    "ua-parser-js": "^0.7.18"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.2",
    "@babel/core": "^7.0.0-rc.2",
    "@babel/preset-env": "^7.0.0-rc.2",
    "prettier": "^1.14.2"
  }
}
